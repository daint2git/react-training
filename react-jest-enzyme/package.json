{
  "name": "react-jest-enzyme",
  "version": "1.0.0",
  "description": "React16 + Jest + Enzyme",
  "main": "index.js",
  "author": "daint2@fsoft.com.vn",
  "license": "ISC",
  "scripts": {
    "start": "webpack-dev-server --hot",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:clean": "rimraf ./coverage",
    "test:coverage": "yarn test:clean && cross-env JEST_ENV=coverage jest",
    "test:storybook": "start-storybook -p 1234 --quiet",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test:unit"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --print-width 100 --semi false --single-quote --trailing-comma all --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^5.0.10",
    "autoprefixer": "^9.4.10",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "husky": "^2.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "jest-styled-components": "^6.3.1",
    "jest-watch-typeahead": "^0.3.0",
    "lint-staged": "^8.1.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-simple-vars": "^5.0.2",
    "raf": "^3.4.1",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^6.1.2",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@babel/runtime-corejs2": "^7.3.4",
    "axios": "^0.18.0",
    "common-tags": "^1.8.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.8.0",
    "styled-components": "^4.1.3"
  }
}
